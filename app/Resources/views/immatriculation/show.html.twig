{% extends '::base.html.twig' %}
{% block ariane -%}
    <ol class="breadcrumb">
          <li class="breadcrumb-item"><a href="{{ path('homepage') }}">Accueil</a></li>
          <li class="breadcrumb-item"><a href="{{ path('admin_parametres_immatriculation_index') }}">Immatriculations</a></li>
          <li class="breadcrumb-item">Détail</li>
    </ol>
{% endblock %}
{% block pagetitle -%}VOIR UNE IMMATRICULATION{% endblock %}
{% block actions %}
    <label class="btn btn-outline-secondary"><a href="{{ path('admin_parametres_immatriculation_index') }}">Liste des types d'immatriculation</a></label>
    <label class="btn btn-outline-secondary"><a href="{{ path('admin_parametres_immatriculation_edit', { 'id': immatriculation.id }) }}">Modifier</a></label>
    <label class="btn btn-outline-secondary">
        {{ form_start(delete_form) }}
            <input type="submit" value="Supprimer" onclick="return confirm('Etes-vous sûr de vouloir supprimer?');">
        {{ form_end(delete_form) }}
    </label>
{% endblock %}
{% block body %}
    <table class="table table-hover">
        <tbody>
            <tr>
                <th>Code</th>
                <td>{{ immatriculation.code }}</td>
            </tr>
            <tr>
                <th>Créé par</th>
                <td>{{ immatriculation.creePar }}</td>
            </tr>
            <tr>
                <th>Modifié par</th>
                <td>{{ immatriculation.modifierPar }}</td>
            </tr>
            <tr>
                <th>Date de création</th>
                <td>{% if immatriculation.dateCreation %}{{ immatriculation.dateCreation|date('Y-m-d H:i:s') }}{% endif %}</td>
            </tr>
            <tr>
                <th>Date de dernière modification</th>
                <td>{% if immatriculation.dateModification %}{{ immatriculation.dateModification|date('Y-m-d H:i:s') }}{% endif %}</td>
            </tr>
        </tbody>
    </table>
{% endblock %}
