{% extends '::base.html.twig' %}

{% block ariane -%}<a href="{{ path('homepage') }}">Accueil</a> > Parametres > <a href="{{ path('typebien_index') }}">Type de bien</a> > Détail{% endblock %}
{% block pagetitle -%}DETAIL DU TYPE DE BIEN{% endblock %}
{% block menu_parametres_actif %}active{% endblock %}
{% block menu_typeBien_actif %}active{% endblock %}

{% block body -%}
    <table cellpadding="0" cellspacing="0" border="0" class="table table-striped table-bordered">
        <tbody>
            <tr>
                <th>Code</th>
                <td>{{ typeBien.code }}</td>
            </tr>
            <tr>
                <th>Libellé</th>
                <td>{{ typeBien.libelle }}</td>
            </tr>
            <tr>
                <th>Créé par</th>
                <td>{{ typeBien.creePar }}</td>
            </tr>
            <tr>
                <th>Modifié par</th>
                <td>{{ typeBien.modifierPar }}</td>
            </tr>
            <tr>
                <th>Date de création</th>
                <td>{% if typeBien.dateCreation %}{{ typeBien.dateCreation|date('Y-m-d H:i:s') }}{% endif %}</td>
            </tr>
            <tr>
                <th>Date de modification</th>
                <td>{% if typeBien.dateModification %}{{ typeBien.dateModification|date('Y-m-d H:i:s') }}{% endif %}</td>
            </tr>
            <tr>
                <th colspan='2'><a href="{{ path('typebien_index') }}">Retour à la liste</a></th>
            </tr>
        </tbody>
    </table>


{% endblock %}
